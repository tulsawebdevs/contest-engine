<%= form_tag "/contest", :multipart => true do %>
  title <input type="" name="title" />
  description <input type="text" name="description" />
  rules <%= file_field_tag "rules", :accept => 'application/pdf', :class => "upload"%>
  sponsor 
  start_date <input type="date" name="start_date" />
  end_date <input type="date" name="end_date" />

  Phase 1 (Initial Submissions) End Date: <input type="date" name="phase1_end_date" />
  Phase 2 (Voting Round 1) End Date: <input type="date" name="phase2_end_date" />
  Phase 3 (Fabrication) End Date: <input type="date" name="phase3_end_date" />
  <%= submit_tag "submit" %>
<% end %>
